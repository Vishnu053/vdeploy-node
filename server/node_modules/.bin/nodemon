#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/nodemon@2.0.15/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/node_modules:/mnt/c/Users/vishn/Desktop/playground/node_modules:/mnt/c/Users/vishn/Desktop/node_modules:/mnt/c/Users/vishn/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/nodemon/bin/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/nodemon/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/nodemon@2.0.15/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/.pnpm/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/node_modules:/mnt/c/Users/vishn/Desktop/playground/node_modules:/mnt/c/Users/vishn/Desktop/node_modules:/mnt/c/Users/vishn/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/nodemon/bin/node_modules:/mnt/c/Users/vishn/Desktop/playground/vdeploy-node/server/node_modules/nodemon/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
